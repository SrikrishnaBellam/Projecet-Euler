    "# Question-1\n",
    "\n",
    "def sum_of_multiples(limit):\n",
    "    sum = 0\n",
    "    for i in range(limit):\n",
    "        if(i%3==0 or i%5==0):\n",
    "            sum+=i\n",
    "            \n",
    "    return sum"
   ]
